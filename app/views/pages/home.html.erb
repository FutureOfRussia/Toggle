<% provide(:title, 'Home') %>
<div class="">
	<% if log_in? %>
		<div class="">
				<% @photos.items.each do |item| %>
					<tr>
						<td>
							<%= "#{item.photo_804}" %>
							<img src="<%= "#{item.photo_804}" %>">
						</td>
					</tr>
				<% end %>
			<% end %>
		</div>
	<% else %>
		<h1>Welcome!</h1>
		<%= link_to "login", signin_path %>
	<% end %>
</div>
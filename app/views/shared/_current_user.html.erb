<div class="content">		
	<% $photos.items.each do |item| %>
		<div class="cont-block">
			<img src="<%= item.photo_604 %>" class="cont-img">
		</div>
	<% end %>
	<% if $photos.first.last > 200 %>
		<div class="load-btn" id="load">
			<a href="/load" data-remote="true">Загрузить еще...</a>
		</div>
	<% end %>
</div>